<h3><%= "Top Ten #{media.first.type}s" %></h3>
  <span>
    <% ordered_media = order_by_ranking(media) %>
    <% 10.times do %>
      <% listing = ordered_media.shift %>
      <p>
      <span class="media-name"><%= link_to "#{listing.name}", media_listing_route(listing) %></span>
      <span><%= "Ranked: #{listing.ranking}" %></span>
      </p>
    <% end %>
  </span>
  <p><%= link_to "Here are all the #{media.first.type}s", media_listings_route(media) %></p>

  <p>
    Top ten partial
  </p>
