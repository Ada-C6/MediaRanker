<table class="row">
<% media_type.each do |media| %>
  <tr>
    <td class="small-3 medium-4 large-4 columns">
      <%= button_to "Upvote", send(  "upvote_#{media_class}_path", media), method: :patch, class: "button"%>
    </td>
    <td class="small-9 medium-6 large-4 columns">
      Rank: <%= media.rank %>
    </td>
    <td class="small-12 medium-12 large-4 columns">
      <%= link_to media.title, send( "#{media_class}_path", media) %>
    </td>
  </tr>
  <% end %>
</table>
