<%= form_for @this_item, :url => { method: :patch} do |f| %>
<%= f.label @name %>
<%= f.text_field @name %>

<%= f.label @person %>
<%= f.text_field @person %>

<%= f.label @description %>
<%= f.text_area @description %>

<%= f.hidden_field :rank, :value => 0 %>

  <%= f.submit "Submit" %>
<% end %>
