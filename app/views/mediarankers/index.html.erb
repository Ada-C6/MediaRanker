<h1>Top 10 Movies</h1>

  <p><%=link_to("See more...", movies_path)%></p>

<h1>Top 10 Books</h1>

  <% @books.limit(10).each do |book| %>

      <h2><%= link_to(book.name, book_path(book[:id])) %></h2>
      <h3>Votes: <%=book.votes.to_s%></h3>
  <%end %>

  <p><%=link_to("See more...", books_path)%></p>

<h1>Top 10 Albums</h1>
  <p><%=link_to("See more...", albums_path)%></p>
