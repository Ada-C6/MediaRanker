<h1>MediaRanker</h1>

<h1>Books</h1>
<% @books.each do |book| %>
<h2><%= link_to(book.title, book_path(book.id)) %></h2>
<p>Rank: <%= book.rank %></p>
<% end %>


<h1>Movies</h1>
<% @movies.each do |movie| %>
<h2><%= link_to(movie.title, movie_path(movie.id)) %></h2>
<p>Rank: <%= movie.rank %></p>
<% end %>

<h1>Albums</h1>
<% @albums.each do |album| %>
<h2><%= link_to(album.title, album_path(album.id)) %></h2>
<p>Rank: <%= album.rank %></p>
<% end %>
