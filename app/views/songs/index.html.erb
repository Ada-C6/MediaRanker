<h1>Media Ranker Songs</h1>

<main>
  <% @songs.each do |song| %>
    <article class="one-song row">
      <h4 class="small-10 medium-7 column"><%= link_to song.title, song_path(song.id) %></h4>
      <p class="small-2 medium-2 column right-padding"> Votes: <%= song.rank %></p>
    </article>
  <% end %>
</main>

<p class="new_button"><%= button_to "Add New Song", {controller: 'songs', action: 'new'}, {path: 'new_song', method: :get} %></p>
<p class="media_button"><%= button_to "View All Media", {controller: 'landings', action: 'index'}, {path: 'landings', method: :get} %></p>
