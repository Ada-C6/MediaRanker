<%= form_for @book,
  url: action_name do |f| %>
<ul>
  <li><%= f.label :title %></li>
  <li><%= f.text_field :title %></li>

  <li><%= f.label :written_by %></li>
  <li><%= f.text_field :written_by %></li>

  <li><%= f.label :description %></li>
  <li><%= f.text_field :description %></li>

  <li><%= f.submit %> </li>
</ul>

<% end %>
