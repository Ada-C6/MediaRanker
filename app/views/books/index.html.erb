<table>
  <tr>
    <th>Ranking</th>
    <th>Name</th>
    <th>Upvote</th>
  </tr>
  <% @books.each do |book| %>
    <tr>
      <td><%= "Ranked: #{book.rank}"%></td>
      <td><%= link_to "#{book.name}", book_path(book) %></td>
      <td><%= button_to "Upvote", upvote_book_path(book), method: :patch %></td>
    </tr>
  <% end %>
</table>

