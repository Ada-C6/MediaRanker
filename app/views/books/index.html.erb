
<section class="background_books">
<h1>Books</h1>
<div class="row">
<div class="small-6 medium-4 large- 2columns">
<% @books.each do |book| %>
<article class="tiles">
<h4><%= link_to(book.title, book_path(book.id)) %></h4>
<h5>Rank: <%= book.rank %></h5>
</article>
</div>
</div>
<% end %>
<p><%= link_to("Add Book", new_book_path, class:"success button") %></p>
</section>
