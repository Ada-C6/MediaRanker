<h1>Books</h1>

  <% @books.each do |book| %>
      <h2><%= link_to(book.name, book_path(book[:id])) %></h2>
      <h3>Votes: <%=book.votes.to_s%></h3>
      <%= button_to "UpVote", {action: "up_vote", id: book[:id] }, method: :post %>
  <%end %>

  <%= link_to( "Add a New Book", new_book_path) %>
  <%= link_to( "View All Media", root_path) %>
