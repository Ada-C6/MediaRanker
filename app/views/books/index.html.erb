<h2>Books</h2>
<%= link_to 'Add a new Book', book_new_path %>
<table>
<tr>
  <th>Rank</th>
  <th>Title</th>
  <th>Upvote</th>
  <th colspan="2"></th>
</tr>
<% @books.each do |book| %>
    <tr>
      <td><%= book.rank %> </td>
      <td><%= link_to book.title, book_show_path(book.id) %></td>
      <td><%# link_to 'Upvote!', market_show_path(market) %></td
    </tr>
  <% end %>
</table>

<%= link_to 'Show All Media', index_path %>
