<table>
<th>Ranking</th>
<th>Name</th>
<th>Upvote</th>

<% @books.each do |book| %>
  <tr>
    <td>Ranked: <%= book.ranking %></td>
    <td><%= link_to(book.name, book_path(book.id)) %></td>
    <td><%= button_to("Upvote", book_upvote_path(book.id), method: :patch) %></td>
  </tr>
<% end %>
</table>

<%= button_to("View All Media", root_path, method: :get) %><%= button_to("Add a Book", new_book_path, method: :get) %>
