
<%= form_for item do |f| %>
  <%= f.label "Name" %>
  <%= f.text_field :name %>
  <%= f.label "Created By" %>
  <%= f.text_field :creator %>
  <%= f.label "Description" %>
  <%= f.text_area :description %>
  <%= f.submit :save %>
<% end %>


<h1><%=link_to "#{item.name}", item, method: :get  %></h1>
