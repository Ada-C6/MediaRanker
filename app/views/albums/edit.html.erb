<h2>Edit Album</h2>

<%= form_for @album, method: :patch do |f| %>
  <p><%= f.label :name %>
  <%= f.text_field :name %></p>

  <p><%= f.label :artist %>
  <%= f.text_field :artist %></p>

  <p><%= f.label :description %>
  <%= f.text_field :description %></p>

  <p><%= f.submit "Save" %></p>
<% end %>
