<h2>Edit Album</h2>

  <%= form_for  @album do |f| %>
    <p><%= f.label :Name  %></p>
    <p><%= f.text_field :name %></p>
    <p><%= f.label :Artist  %></p>
    <p><%= f.text_field :artist %></p>
    <p><%= f.label :Description  %></p>
    <p><%= f.text_field :description %></p>
    <p> <%= f.submit "Save" %></p>
  <% end %>
