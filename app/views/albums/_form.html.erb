<%= form_for @album, method: http_method do |f| %>
  <br><b><%= f.label :name, " Name" %></b>
  <br><%= f.text_field :name %>

  <br><b><%= f.label :artist %></b>
  <br><%= f.text_field :artist %>

  <br><b><%= f.label :description, "Description" %></b>
  <br><%= f.text_area :description %>
  <br><%= f.submit "Save"%>
<% end %>
