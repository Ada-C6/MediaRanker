<h1><%= @album.title %></h1>
<h3>Artist: <%= @album.artist %></h3>
<p><%= @album.description %></p>
<p>Ranked: <%= @album.rank %></p>

<%= link_to 'Edit Album', album_edit_path(@album.id) %>
<%= button_to "Delete Album", album_delete_path(@album.id), method: :delete, data: {confirm:"Are you Sure?" }  %>

<%= link_to 'Show All Media', index_path %>
