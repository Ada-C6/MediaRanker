<%= render "medias/shared_display", title: @album, created_verb: "Recorded", maker: @album.artist, rank: @album, about: @album %>

<%= button_to "UPVOTE", upvote_album_path(@album), method: :patch  %>
<%= link_to "Edit #{@album.name}", edit_album_path %>
<%= link_to "DELETE", album_path(@album.id), method: :delete,
data: { confirm: 'Are you sure?'} %>

<%= link_to "View All Media", root_path%>
<%= link_to "View All Albums", albums_path %>
