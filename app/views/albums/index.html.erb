<h1>Albums#index</h1>
<p>Find me in app/views/albums/index.html.erb</p>
<h1>Top Albums</h1>
<table>
  <tr>
    <th>votes</th>
    <th>name</th>
    <th>Upvote</th>
  </tr>
  <% @albums.each do |book| %>
  <tr>
    <td>Ranked: <%=book.vote_count %></td>
    <td><%=link_to book.name, book_path(book.id) %></td>
    <td><%=button_to("Upvote", books_upvote_path(book.id), method: :patch) %></td>
  </tr>
  <% end %>
</table>
