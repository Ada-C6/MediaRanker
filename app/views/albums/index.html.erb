<% @albums.each do |album| %>
<%= "#{album.rank}" %>
  <%= link_to("#{album.title}", album_path(album)) %>
  <%= button_to("Upvote", album_upvote_path(album), method: :patch) %>
<% end %>

<%= button_to("View all media", root_path, method: :get) %>
<%= button_to("Add an album", new_album_path, method: :get) %>
