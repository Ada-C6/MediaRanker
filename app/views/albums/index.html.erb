<h1> All the albums. </h1>
<table>
<% @albums.each do |album| %>
  <tr>
    <td>
      Rank: <%= album.rank %>
    </td>
    <td>
      <%= link_to album.title, album_path(album) %>
    </td>
    <td>
      <%= button_to "Upvote",   upvote_album_path(album), method: :patch %>
    </td>
  </tr>
<% end %>
</table>

<%= button_to "Add new album", new_album_path, method: :get%>
