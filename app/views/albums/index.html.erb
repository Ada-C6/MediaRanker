<table>
  <tr>
    <th>Ranking</th>
    <th>Name</th>
    <th>Upvote</th>
  </tr>
  <% @albums.each do |album| %>
  <tr>
    <td><%= "Ranked: #{album.rank}"%></td>
    <td><%= link_to "#{album.name}", album_path(album) %></td>
    <td><%= button_to "Upvote", upvote_album_path(album), method: :patch %></td>
  </tr>
  <% end %>
</table>

