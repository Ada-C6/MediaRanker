<table>
<th>Ranking</th>
<th>Name</th>
<th>Upvote</th>

<% @albums.each do |album| %>
  <tr>
    <td>Ranked: <%= album.ranking %></td>
    <td><%= link_to(album.name, album_path(album.id)) %></td>
    <td><%= button_to("Upvote", album_upvote_path(album.id), method: :patch) %></td>
  </tr>
<% end %>
</table>

<%= button_to("View All Media", root_path, method: :get) %><%= button_to("Add a Album", new_album_path, method: :get) %>
