<h2>All Albums</h2>
<table class="index-table">
  <tr>
    <th>Album Name</th>
    <th>Actions</th>
  </tr>
  <% @albums.each do |album| %>
  <tr>
    <td><%= link_to "#{album.name}", album_path(album) %></td>
    <td><small>
      <%= link_to "[Edit]", edit_album_path(album) %>
      <%= link_to "[Delete]", album_path(album), method: :delete, data: { confirm: "Really, now?" } %></small> </td>
  </tr>
  <% end %>
</table>
