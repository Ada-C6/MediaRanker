<h2>Albums</h2>
<%= link_to 'Add an Album', album_new_path %>
<table>
<tr>
  <th>Rank</th>
  <th>Title</th>
  <th>Upvote</th>
  <th colspan="2"></th>
</tr>
<% @albums.each do |album| %>
    <tr>
      <td><%= album.rank %> </td>
      <td><%= link_to album.title, album_show_path(album.id) %></td>
      <td><%# link_to 'Upvote!', market_show_path(market) %></td
    </tr>
  <% end %>
</table>

<%= link_to 'Show All Media', index_path %>
