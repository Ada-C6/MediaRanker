<h1>Albums#index</h1>
<p>Find me in app/views/albums/index.html.erb</p>


<article>
  <h1>ranking</h1> <h1>name</h1> <h1>upvote</h1>
  <% @albums.each do |album| %>
  <p>
    ranked: album.votes
  </p>
  <p>
    <%= link_to album.name, album_path(album.id) %>
  </p>
  <p>
    <%= button_to 'upvote', {action: "upvote", id: album.id }, method: :patch %>
  </p>
  <% end %>
  <p>
    <%= link_to 'Media Ranker', root_path %>
  </p>
  <p>
    <%= link_to 'add new', new_album_path %>
  </p>
</article>
