<div class ="row">
  <div class="small-up-12 medium-up-12 large-up-12">
    <section>
      <table>
        <th>Ranking</th>
        <th>Name</th>
        <th>Upvote</th>
        <% @albums.each do |album| %>
        <tr>
          <td>Ranked: <%= album.ranking %></td>
          <td><%= link_to(album.name, album_path(album.id)) %></td>
          <td><%= link_to("Upvote", album_upvote_path(album.id), method: :patch, class: "button secondary radius") %></td>
        </tr>
        <% end %>
      </table>
      <%= link_to("View All Media", root_path, method: :get, class: "button secondary radius") %> <%= link_to("Add a Album", new_album_path, method: :get, class: "button secondary radius") %>
    </section>
  </div>
</div>
