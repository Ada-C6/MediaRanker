<h2>Edit Market</h2>

<%= form_for @this_item, :url => { method: :patch} do |f| %>
<%= f.label :name %>
<%= f.text_field :name %>

<%= f.label :director %>
<%= f.text_field :director %>

<%= f.label :description %>
<%= f.text_area :description %>

<%= f.hidden_field :rank, :value => 0 %>

  <%= f.submit "Submit" %>
<% end %>
