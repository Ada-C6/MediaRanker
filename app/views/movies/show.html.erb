<h3><%= "#{@movie.title}" %></h3>
<h4><%= "Directed by #{@movie.director}" %></h4>
<p>
  <%= "#{@movie.description}" %>
</p>
<p>
  <%= "Ranked: #{@movie.vote}" %>
</p>


<%= form_for @movie, method: :patch do |f|%>
  <%= f.hidden_field :vote, :value => @movie.upvote %>
  <%= f.submit "Upvote!" %>
<% end %>
<%= link_to "Edit Movie", edit_movie_path %>
<%= link_to "Home", root_path %>
<%= link_to "All Movies", movies_path %>
