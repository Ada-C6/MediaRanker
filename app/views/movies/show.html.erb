
<h1><%= @movie.name %></h1>
<h3>Directed By: <%=@movie.creator%></h3>
<div class=rank><%= @movie.rank %></div>
<p>
  <%= @movie.description%>
</p>


<%=  button_to("Upvote ", action: "upvote", method: "post", id: @movie.id) %>

<%=  button_to("Edit #{@movie.name}", edit_movie_path(id: @movie.id), {method: :get, class: 'btn'} )%>
<%=  button_to("View all #{params[:controller]} ", action: "index", method: :get, id: @movie.id) %>
<%=  button_to("View all media", index_path) %>


<%= params %>
