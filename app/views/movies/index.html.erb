<h1>Movies</h1>

<% @movie_list_by_votes.each do |m| %>
<ul class = 'list'>
  <li><%= " #{m.votes}" %>
  <%= link_to "#{m.title}", movie_path(m.id)%>
  <%= button_to "Upvote", upvote_movie_path(m), method: :patch %></li>
</ul>
<% end %>

<%= link_to "Add Movie", new_movie_path %>
<%= link_to "Return to All Media", main_index_path %>
