<h1>All Movies</h1>
<table>
  <tr>
    <th>Ranking</th>
    <th>Name</th>
    <th>Upvote</th>
  </tr>

  <% @movies.each do |m| %>
    <tr>
      <td><%= "Ranked: #{m.ranking}" %></td>
      <td><%= m.name %></td>
      <td><%= link_to "Upvote", upvote_movie_path(m), method: :patch %></td>
    </tr>
  <% end %>

</table>
