<h1>Viewing All Movies </h1>

<h3> Ranking || Name || Upvote </h3>
<% @all_movies.each do |movie| %>
<p><%= "#{movie.ranking} |------------|" %>
<%=  link_to "#{movie.name}", movie_path(movie) %>
<%= button_to "UPVOTE", upvote_movie_path(movie), method: :patch  %> </p>
<% end %>


<%= link_to "View All Media", root_path%>
<%= link_to "Add a Movie", new_movie_path%>
