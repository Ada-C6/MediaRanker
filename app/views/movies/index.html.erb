<!-- movie index  -->
  <center><h1>All Movies</h1></center>
  <table>
    <tr>
      <th>votes</th>
      <th>name</th>
      <th>Upvote</th>
    </tr>
    <% @movies.each do |movie| %>
    <tr>
      <td>Ranked: <%=movie.vote_count %></td>
      <td><%=link_to movie.name, movie_path(movie.id) %></td>
      <td><%=button_to("Upvote", movies_upvote_path(movie.id), method: :patch) %></td>
    </tr>
    <% end %>
</table>

<section class="button_row">
 <p><%=button_to("Add Movie", new_movie_path, method: :get)%></p>
 <p><%=button_to "View All Media", users_path, method: :get %></p>
</section>
