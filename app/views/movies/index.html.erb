<table>
  <tr>
    <th>Ranking</th>
    <th>Name</th>
    <th>Upvote</th>
  </tr>
  <% @movies_by_rank.each do |movie| %>
    <tr>
      <td><%= " #{movie.rank}" %></td>
      <td><%= link_to "#{movie.name}", movie_path(movie.id)%></td>
      <td><%= button_to "Upvote", upvote_movie_path(movie), method: :patch %></td>
    </tr>
  <% end %>
</table>

<h4><%= link_to "Add a Movie", new_movie_path %>  <%= link_to "View All Media", homes_index_path %></h4>
