<h2>All Movies</h2>

<table class="index-table">
  <tr>
    <th>Points</th>
    <th>Movie Name</th>
    <th>More...</th>
  </tr>
  <% @movies.each do |movie| %>
  <tr>
    <td><%= "#{movie.rank_points}" %></td>
    <td><%= "#{movie.name}" %></td>
    <td><small>
      <%= link_to "[Details]", movie_path(movie) %>
  </tr>
  <% end %>
</table>
